{
    "name": "Response Memorizer",
    "description": "Memorizes Responses and auto-responses to same requests",
    "version": "0.0.1",
    "manifest_version": 3,
    "background": {
        "service_worker": "background.js"
    },
    "action": {
        "default_popup": "popup.html"
    },
    "permissions": ["debugger", "tabs", "storage"]
  }
